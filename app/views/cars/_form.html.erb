<%= simple_form_for(@car) do |f| %>

  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :price_per_day %>
  <%= f.input :address %>
  <%= f.input :km %>
  <%= f.input :year %>
  <%= f.input :seats %>
  <%= f.input :carburant %>
  <%= f.input :doors %>
  <%= f.input :power %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create the car"%>
<% end %>
